{
    "name": "@flowscripter/ts-template",
    "version": "0.0.0",
    "description": "Template TypeScript project.",
    "repository": {
        "type": "git",
        "url": "https://github.com/flowscripter/ts-template"
    },
    "homepage": "https://github.com/flowscripter/ts-template",
    "bugs": {
        "url": "https://github.com/flowscripter/ts-template/issues"
    },
    "keywords": [
        "typescript"
    ],
    "contributors": [
        {
            "name": "Vectronic",
            "email": "hello.vectronic@gmail.com",
            "url": "https://vectronic.io"
        }
    ],
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "main": "dist/index.mjs",
    "module": "dist/index.mjs",
    "files": [
        "dist/**/*"
    ],
    "typings": "dist/types/index.d.ts",
    "engines": {
        "node": ">=10.15.1"
    },
    "scripts": {
        "prebuild": "npx rimraf dist",
        "build": "npx rollup -c",
        "watch": "npx rollup -c --watch",
        "test": "npx jest",
        "lint": "npx eslint ./src/**/*.ts ./test/**/*.ts",
        "docs": "npx typedoc --moduleResolution node src"
    },
    "devDependencies": {
        "@commitlint/cli": "8.0.0",
        "@commitlint/config-conventional": "8.0.0",
        "@flowscripter/eslint-config": "1.2.8",
        "@flowscripter/tsconfig": "1.1.0",
        "@types/debug": "4.1.4",
        "@types/jest": "24.0.15",
        "@typescript-eslint/eslint-plugin": "1.11.0",
        "@typescript-eslint/parser": "1.11.0",
        "eslint": "5.16.0",
        "eslint-plugin-import": "2.18.0",
        "eslint-plugin-jest": "22.7.1",
        "gulp": "4.0.2",
        "husky": "2.5.0",
        "jest": "24.8.0",
        "rimraf": "2.6.3",
        "rollup": "1.16.2",
        "rollup-plugin-cleanup": "3.1.1",
        "rollup-plugin-commonjs": "10.0.0",
        "rollup-plugin-eslint": "6.0.0",
        "rollup-plugin-json": "4.0.0",
        "rollup-plugin-node-resolve": "5.1.0",
        "rollup-plugin-peer-deps-external": "2.2.0",
        "rollup-plugin-typescript2": "0.21.2",
        "rollup-watch": "4.3.1",
        "semantic-release": "15.13.16",
        "sonarqube-scanner": "2.4.1",
        "temp-dir": "2.0.0",
        "ts-jest": "24.0.2",
        "typedoc": "0.14.2",
        "typedoc-plugin-external-module-name": "2.1.0",
        "typescript": "3.5.2"
    },
    "peerDependencies": {
        "debug": "^4.1.1"
    },
    "dependencies": {}
}
