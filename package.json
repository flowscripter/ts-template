        "rollup-plugin-typescript2": "0.26.0",
        "semantic-release": "17.0.4",
        "sonarqube-scanner": "2.6.0",
        "temp-dir": "2.0.0",
        "ts-jest": "25.2.1",
        "typedoc": "0.17.3",
        "typedoc-plugin-external-module-name": "3.0.0",
        "typescript": "3.8.3"
    },
